<?xml version="1.0"?>

<PropertyList>
  <generic>
    <output>
      <line_separator>newline</line_separator>
      <var_separator>tab</var_separator>
      <chunk>
        <name>time (sec)</name>
        <type>float</type>
        <format>%.4f</format>
        <node>/sim/time/elapsed-sec</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/orientation/pitch-deg</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/orientation/roll-deg</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/orientation/true-heading-deg</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/position/altitude-ft</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/position/global-x</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/position/global-y</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/position/global-z</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/velocities/true-airspeed-kt</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft/velocities/vertical-speed-fps</node>
      </chunk>
    </output>
  </generic>
</PropertyList>