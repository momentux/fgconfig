<?xml version="1.0"?>

<!-- Example usage: <dash><dash>generic=file,out,4,fgfs.gps,insgns-gps -->

<PropertyList>
  <generic>
    <output>
      <line_separator>newline</line_separator>
      <var_separator>tab</var_separator>
      <chunk>
        <name>time (sec)</name>
        <type>float</type>
        <format>%.4f</format>
        <node>/sim/time/elapsed-sec</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/orientation/pitch-deg</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/orientation/roll-deg</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/orientation/true-heading-deg</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/position/altitude-ft</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/position/global-x</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/position/global-y</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/position/global-z</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/velocities/true-airspeed-kt</node>
      </chunk>
      <chunk>
        <type>double</type>
        <node>/ai/models/aircraft[1]/velocities/vertical-speed-fps</node>
      </chunk>
    </output>
  </generic>
</PropertyList>